{
  "name": "cloud-summit-2025",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run local:dev",
    "start": "npm run prod:start",
    "build": "npm run prod:build",
    "lint": "npm run local:lint",
    
    "local:dev": "next dev",
    "local:start": "next start",
    "local:build": "next build",
    "local:lint": "next lint",
    
    "local:setup": "npm install && npm run local:db:stop && npm run local:db:setup && npm run local:db:push && npm run local:db:seed",
    "local:db:start": "brew services start mongodb-community",
    "local:db:stop": "brew services stop mongodb-community",
    "local:db:restart": "brew services restart mongodb-community",
    "local:db:setup": "cross-env APP_ENV=development ./scripts/setup-mongodb.sh",
    "local:db:push": "cross-env APP_ENV=development prisma db push",
    "local:db:seed": "cross-env APP_ENV=development ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "local:db:clean": "cross-env APP_ENV=development ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts --clean",
    "local:db:reseed": "npm run local:db:clean && npm run local:db:seed",
    "local:db:studio": "cross-env APP_ENV=development prisma studio",
    
    "staging:build": "cross-env APP_ENV=staging NEXT_PHASE=build prisma generate && next build",
    "staging:start": "cross-env APP_ENV=staging next start -p ${PORT:-3000}",
    "staging:setup": "cross-env APP_ENV=staging prisma generate && npm run staging:db:push && npm run staging:db:seed",
    "staging:db:push": "cross-env APP_ENV=staging prisma db push",
    "staging:db:seed": "cross-env APP_ENV=staging ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "staging:db:clean": "cross-env APP_ENV=staging ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts --clean",
    "staging:db:reseed": "cross-env APP_ENV=staging prisma generate && cross-env APP_ENV=staging prisma db push && npm run staging:db:clean && npm run staging:db:seed",

    "prod:build": "cross-env APP_ENV=production next build",
    "prod:start": "cross-env APP_ENV=production next start"
  },
  "dependencies": {
    "@next/third-parties": "^15.1.6",
    "@prisma/client": "^6.3.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@types/lodash": "^4.17.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "autoprefixer": "^10.4.20",
    "caniuse-lite": "^1.0.30001697",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "debounce": "^2.2.0",
    "embla-carousel-autoplay": "^8.5.2",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^11.11.17",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.438.0",
    "mongodb": "^6.13.0",
    "next": "^14.2.24",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.45",
    "postcss-import": "^16.1.0",
    "prisma": "^6.3.0",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
